{
  "name": "swagger-wiki",
  "description": "A cli to create wiki pages in a project based on swagger files.",
  "version": "1.0.16",
  "author": {
    "name": "Ben Force",
    "email": "bforce@teamdrg.com"
  },
  "bin": {
    "swagger-wiki": "./bin/run"
  },
  "bugs": "https://github.com/drg-adaptive/swagger-wiki/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.2",
    "@oclif/plugin-help": "^2",
    "axios": "^0.19.0",
    "doctoc": "^1.4.0",
    "dotenv": "^8.0.0",
    "execa": "^2.0.3",
    "graphql-markdown": "^5.0.1",
    "js-yaml": "^3.13.1",
    "listr": "^0.14.3",
    "swagger-markdown": "github:syroegkin/swagger-markdown",
    "tslib": "^1",
    "zen-observable": "^0.8.14"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/tslint": "^3",
    "@types/dotenv": "^6.1.1",
    "@types/js-yaml": "^3.12.1",
    "@types/listr": "^0.14.2",
    "@types/node": "^10",
    "@types/yaml": "^1.0.2",
    "@types/zen-observable": "^0.8.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "3.0.2",
    "globby": "^10",
    "ts-node": "^8",
    "tslint": "^5",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/drg-adaptive/swagger-wiki",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "swagger-wiki",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/drg-adaptive/swagger-wiki",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p . -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "build": "echo NO BUILD",
    "postversion": "git push --follow-tags",
    "commit": "yarn git-cz"
  },
  "types": "lib/index.d.ts",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
